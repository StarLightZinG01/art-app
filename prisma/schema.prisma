generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model image {
  id          Int    @id @default(autoincrement())
  description String @default("ยังไม่ได้กรอกรายละเอียด") @db.Text
  url         String @db.VarChar(500)
  name_pic    String @default("ไม่มีชื่อ") @db.VarChar(100)
}
